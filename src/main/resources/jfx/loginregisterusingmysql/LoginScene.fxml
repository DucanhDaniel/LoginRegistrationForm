<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchor" prefHeight="409.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfx.loginregisterusingmysql.LoginController">
   <children>
      <Label alignment="CENTER" layoutY="59.0" prefHeight="34.0" prefWidth="401.0" text="LOGIN FORM">
         <font>
            <Font name="Lucida Console" size="25.0" />
         </font>
      </Label>
      <Label alignment="BASELINE_LEFT" layoutX="30.0" layoutY="114.0" prefHeight="44.0" prefWidth="370.0" text="Username:">
         <font>
            <Font name="Lucida Console" size="17.0" />
         </font>
      </Label>
      <Label alignment="BASELINE_LEFT" layoutX="32.0" layoutY="170.0" prefHeight="44.0" prefWidth="370.0" text="Password:">
         <font>
            <Font name="Lucida Console" size="17.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordTextField" layoutX="135.0" layoutY="171.0" prefHeight="44.0" prefWidth="244.0" promptText="Password">
         <font>
            <Font name="Lucida Console" size="17.0" />
         </font>
      </PasswordField>
      <Button fx:id="loginBtn" alignment="CENTER" layoutX="135.0" layoutY="237.0" mnemonicParsing="false" onAction="#loginBtnController" prefHeight="31.0" prefWidth="244.0" text="Login!">
         <font>
            <Font name="Lucida Console" size="13.0" />
         </font>
      </Button>
      <Label alignment="BASELINE_LEFT" layoutX="15.0" layoutY="280.0" prefHeight="44.0" prefWidth="200.0" text="Do not have an account?">
         <font>
            <Font name="Lucida Console" size="14.0" />
         </font>
      </Label>
      <Button fx:id="registerBtn" alignment="CENTER" layoutX="215.0" layoutY="286.0" mnemonicParsing="false" onAction="#registerBtnController" prefHeight="31.0" prefWidth="164.0" text="Register now!">
         <font>
            <Font name="Lucida Console" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="usernameTextField" layoutX="135.0" layoutY="114.0" prefHeight="44.0" prefWidth="244.0" promptText="Username ">
         <font>
            <Font name="Lucida Console" size="17.0" />
         </font>
      </TextField>
      <Label fx:id="messageLabel" alignment="CENTER" layoutX="5.0" layoutY="324.0" prefHeight="44.0" prefWidth="393.0" textFill="RED">
         <font>
            <Font name="Lucida Console" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
